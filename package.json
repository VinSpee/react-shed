{
  "name": "react-shed",
  "version": "3.0.0-alpha.3",
  "description": "Shed.css as a react styled-component",
  "main": "lib/index.js",
  "files": [
    "lib",
    "umd"
  ],
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-plugin-shed": "^1.0.0",
    "glamor": "^2.20.25",
    "glamorous": "^3.18.1",
    "modularscale": "^2.0.1",
    "nwb": "^0.15.8",
    "prop-types": "^15.5.10",
    "react": "^15.5.3",
    "react-dom": "^15.5.3"
  },
  "peerDependencies": {
    "glamor": "2.x.x",
    "glamorous": "3.x.x",
    "react": "^15"
  },
  "scripts": {
    "start": "nwb serve",
    "build": "nwb build react-component",
    "prepublish": "yarn build"
  },
  "eslintConfig": {
    "env": {
      "jest": true,
      "browser": true
    },
    "globals": {
      "it": true,
      "describe": true,
      "test": true,
      "expect": true,
      "react": true,
      "mount": true,
      "shallow": true,
      "render": true,
      "React": true
    },
    "parser": "babel-eslint",
    "extends": [
      "airbnb"
    ],
    "rules": {
      "react/no-unused-prop-types": 0,
      "no-debugger": 1,
      "no-mixed-operators": 0,
      "no-nested-ternary": 0,
      "no-confusing-arrow": 0,
      "react/jsx-filename-extension": 0,
      "import/extensions": 0,
      "import/no-named-as-default-member": 0,
      "import/no-named-as-default": 0,
      "import/no-unresolved": 0,
      "import/no-extraneous-dependencies": 0,
      "import/no-absolute-path": 0,
      "import/no-duplicates": 0,
      "react/jsx-no-undef": 0,
      "no-unused-vars": [
        "error",
        {
          "varsIgnorePattern": "Shed"
        }
      ]
    }
  },
  "author": "Vince Speelman",
  "license": "MIT",
  "dependencies": {
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "color": "1.0.3",
    "curry": "^1.2.0",
    "eslint": "3.19.x",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "2.2.x",
    "eslint-plugin-jsx-a11y": "5.0.x",
    "eslint-plugin-react": "^7.0.1"
  }
}
